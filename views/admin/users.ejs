<!DOCTYPE html>
<html lang="en">
<head>
   

    <%- include('../partials/admin_head.ejs')%>  
 
    <title>users</title>
</head>
<body>
    <header>
        <%- include('../partials/admin_navbar1.ejs')%>
        <%- include('../partials/user_navbar2.ejs')%>
    </header>
    <main>
<div class="container">


      <table>
        <thead>
          <tr>
              <th>id</th>
              <th>Username</th>
              <th>Password</th>
              <th>phone</th>
              <th>Email</th>
              <th>User type</th>
              <th>edit</th>
              <th>delete</th>
          </tr>
        </thead>

        <tbody>
            <% users.forEach(user=>{ %>
          <tr>
            <td><%= user.id%></td>
            <td><%= user.username%></td>
            <td><%= user.password%></td>
            <td><%= user.phone%></td>
            <td><%= user.email%></td>
            <td><%= user.user_type%></td>
            <td><a href="/admin/edit/<%= user.id%>">Edit</a></td>
            <td><a href="/admin/delete/<%= user.id%>">Delete</a></td>
          </tr>
             
       <% }) %> 
        </tbody>
      </table>
    </div>
    </main>
   

   <%- include('../partials/footer.ejs')%>
</body>
<%- include("../partials/admin_foot.ejs") %>
</html>